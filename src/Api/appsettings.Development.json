{
    "ConnectionStrings": {
        "PostgresConnection": "Server=localhost;Port=5432;Database=codungeons;User Id=postgres;Password=mysecretpassword"
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console"
        ],
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "System": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithExceptionDetails"
        ]
    }
}